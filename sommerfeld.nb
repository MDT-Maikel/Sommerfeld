             RowBox[{"x_", ",", "y_"}], "]"}], "\[RuleDelayed]", 
            RowBox[{"\"\<(\>\"", "<>", 
             RowBox[{"ToString", "[", 
              RowBox[{"ExpressionToC", "[", "x", "]"}], "]"}], "<>", 
             "\"\< / \>\"", "<>", 
             RowBox[{"ToString", "[", 
              RowBox[{"ExpressionToC", "[", "y", "]"}], "]"}], "<>", 
             "\"\<)\>\""}]}]}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Handle", " ", "heads", " ", "with", " ", "arguments"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"parts", "=", 
       RowBox[{"Level", "[", 
