Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "ExpressionToC", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ExpressionToC", "[", "expr_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"newexpr", ",", "parts"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"newexpr", "=", "expr"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"First", " ", "handle", " ", "simple", " ", "heads"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"expr", "===", "\[Pi]"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Return", "[", "\"\<M_PI\>\"", "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"expr", "===", "\[Alpha]s"}], ",", 
        RowBox[{
         RowBox[{"Return", "[", "\"\<alpha_s\>\"", "]"}], ";"}]}], "]"}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"expr", "===", "\[Alpha]sommerfeld"}], ",", 
        RowBox[{
         RowBox[{"Return", "[", "\"\<alpha_sommerfeld\>\"", "]"}], ";"}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "expr", "]"}], "===", "Integer"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Return", "[", 
          RowBox[{"StringReplace", "[", 
           RowBox[{
            RowBox[{"ToString", "[", "newexpr", "]"}], ",", 
            RowBox[{
             RowBox[{"a", ":", 
              RowBox[{"RegularExpression", "[", "\"\<[0-9]+\>\"", "]"}]}], 
             "\[RuleDelayed]", 
             RowBox[{"a", "<>", "\"\<.0\>\""}]}]}], "]"}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "expr", "]"}], "===", "Symbol"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Return", "[", 
          RowBox[{"ToString", "[", "newexpr", "]"}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "expr", "]"}], "===", "Real"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Return", "[", 
          RowBox[{"ToString", "[", "newexpr", "]"}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "expr", "]"}], "===", "Rational"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Return", "[", 
          RowBox[{"expr", "/.", 
           RowBox[{
